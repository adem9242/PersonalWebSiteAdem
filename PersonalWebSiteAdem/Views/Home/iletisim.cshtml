    <section class="paralax-mf footer-paralax bg-image sect-mt4 route" style="background-image: url(../../img/iletisimbg.jpg)">
        <div class="overlay-mf"></div>
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="contact-mf">

                        <div id="contact" class="box-shadow-full">

                            <div class="row">

                                <div class="col-md-6">
                                    <div class="title-box-2">
                                        <h5 class="title-left">
                                            Bana mesaj gönder
                                        </h5>
                                    </div>
                                    <div>
                                        <form action="" href="" method="post" role="form">
                                            <div id="sendmessage">Your message has been sent. Thank you!</div>
                                            <div id="errormessage"></div>
                                            <div class="row">
                                                <div class="col-md-12 mb-3">
                                                    <div class="form-group">
                                                       <input type="text" name="name" class="form-control" id="name" placeholder="Adınız" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                                                        <div class="validation"></div>
                                                    </div>
                                                </div>
                                                <div class="col-md-12 mb-3">
                                                    <div class="form-group">
                                                        <input type="email" class="form-control" name="email" id="email" placeholder="Email" data-rule="email" data-msg="Please enter a valid email" />
                                                        <div class="validation"></div>
                                                    </div>
                                                </div>
                                                <div class="col-md-12 mb-3">
                                                    <div class="form-group">
                                                        <input type="text" class="form-control" name="subject" id="subject" placeholder="Konu" data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                                                        <div class="validation"></div>
                                                    </div>
                                                </div>
                                                <div class="col-md-12 mb-3">
                                                    <div class="form-group">
                                                        <textarea class="form-control" name="message" id="message" rows="5" data-rule="required" data-msg="Please write something for us" placeholder="Mesaj Yaz"></textarea>
                                                        <div class="validation"></div>
                                                    </div>
                                                </div>
                                                <div class="col-md-12">
                                                    <button style="background-color:#6699cc;" type="submit" class="btn btn-outline-success  button-a  button-big">Gönder</button>

                                                </div>
                                                <div>
                                                    <p style="margin-left:30px; margin-top:10px;font-size:19px;color:green;">@ViewBag.dogru</p>
                                                    <p style="margin-left:30px; margin-top:10px;font-size:19px;color:darkred;">@ViewBag.yanlis</p>


                                                </div>
                                                
                                            </div>
                                        </form>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="title-box-2 pt-4 pt-md-0">
                                        <h5 class="title-left">
                                            İletişim
                                        </h5>
                                    </div>
                                    <div class="more-info">
                                        <div class="row">
                                            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1528.1773717366887!2d32.879506969157426!3d40.00052299656542!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14d34d6c0b4f6613%3A0x3d347f09e500c944!2zQWRuYW4gTWVuZGVyZXMsIMWeaHQuIEhha2FuIMOcbGdlciBTay4gTm86NDgsIDA2MzAwIEtlw6dpw7ZyZW4vQW5rYXJh!5e0!3m2!1str!2str!4v1571225577126!5m2!1str!2str" width="100%" height="250" frameborder="0" style="border:0;" allowfullscreen=""></iframe>

                                        </div>
